#include <iostream>

using namespace std;

int main()
{   //Approach :- Divide the number by 10 & then Addsing it using below:
    int N=12345,sum=0;
    while(N>0){
        int rem=N%10;
        sum=sum*10+rem;
        N=N/10;
    }
    cout<<sum;
}
